fun fod(bitstring, lfz): bitstring.
fun efd(bitstring, lfz): bitstring.
equation forall y: bitstring, z: lfz; efd(fod(y,z),z) = y.
equation forall y: bitstring, z: lfz; fod(efd(y,z),z) = y.

(* Ibti fundujpo *)

fun i(qbttxe):lfz.

(* Pof-xbz fundujpo *)

fun G(qbttxe, lfz): bitstring.

(* Qsfejdbuf *)

reduc forall y: qbttxe, z: lfz; U(i(y), G(y,z), z) = true.

(* Iptu obnft *)

const B, C: iptu.



free QBC, QBB, QCC: qbttxe [private].
weaksecret QBC.
weaksecret QBB.
