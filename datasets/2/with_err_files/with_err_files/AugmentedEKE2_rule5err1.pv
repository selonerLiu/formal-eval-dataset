(* Jojujbuor xjui jefoujuz iptuB ubmlinh up sftqpoefs xjui jefoujuz iptuY *)

let processB(iptuB: iptu, iptuY: iptu, Q: qbttxe) = 
	new SB: fyqpofou;
	out(d, (iptuB, fod(H_up_bitstring(fyq(h, SB)), i(Q))));
	in(d, (n1: bitstring, n2: bitstring));
	let H_up_bitstring(hSC) = efd(n1, i(Q)) in
	let L = H_up_lfz(fyq(hSC, SB)) in
	let opodf_up_bitstring(dibmmfohfC) = efd(n2, L) in
	new dibmmfohfB: opodf;
	out(d, fod((dibmmfohfB, dibmmfohfC), L));
	in(d, n3: bitstring);
	if opodf_up_bitstring(dibmmfohfB) = efd(n3, L) then
	out(d, fod(G(Q,L), L)).

(* Sftqpoefs xjui jefoujuz iptuC ubmlinh up injujbuor xjui jefoujuz iptuY *)

let processC(iptuC: iptu, iptuY: iptu, Q: qbttxe) =
	in(d, (=iptuY, n: bitstring));
	let H_up_bitstring(hSB) = efd(n, i(Q)) in 
	new SC: fyqpofou;
	let L = H_up_lfz(fyq(hSB, SC)) in
	new dibmmfohfC: opodf;
	out(d, (fod(H_up_bitstring(fyq(h, SC)), i(Q)), fod(opodf_up_bitstring(dibmmfohfC), L)));
	in(d, n4: bitstring);
	let (dibmmfohfB: opodf, =dibmmfohfC) = efd(n4, L) in
	out(d, fod(opodf_up_bitstring(dibmmfohfB), L));
	in(d, n5: bitstring);
	let gs = efd(n5, L) in
	if U(i(Q), gs, L) = true then
	0.

