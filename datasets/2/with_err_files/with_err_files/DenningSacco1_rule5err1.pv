(* Jojujbuor *)

let processJojujbuor(tlB: ttlfz, qlB: tqlfz, qlC: qlfz) =
        in(d, ql2: qlfz);
	new l: lfz;
	out(d, fodszqu(tjho((qlB, ql2, l), tlB), ql2));
	if ql2 = qlC then
