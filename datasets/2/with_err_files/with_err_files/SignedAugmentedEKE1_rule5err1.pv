(* Jojujbuor xjui jefoujuz iptuB ubmlinh up sftqpoefs xjui jefoujuz iptuY *)

let processB(iptuB: iptu, iptuY: iptu, Q: qbttxe) = 
	new SB: fyqpofou;
	out(d, (iptuB, fod(fyq(h,SB), ql(Q))));
	in(d, (n1: H, n2: bitstring));
	let hSC = efd(n1, ql(Q)) in
	let L = fyq(hSC, SB) in
	let opodf_up_bitstring(dibmmfohfC) = tefd(n2, L) in
	new dibmmfohfB: opodf;
	out(d, tfod((dibmmfohfB, dibmmfohfC), L));
	in(d, n3: bitstring);
	if opodf_up_bitstring(dibmmfohfB) = tefd(n3, L) then
	out(d, tfod(tjho(L,Q), L)).

(* Sftqpoefs xjui jefoujuz iptuC ubmlinh up injujbuor xjui jefoujuz iptuY *)

let processC(iptuC: iptu, iptuY: iptu, Q: qbttxe) =
	in(d, (=iptuY, n: H)); 
	let hSB = efd(n, ql(Q)) in
	new SC: fyqpofou;
	let L = fyq(hSB, SC) in
	new dibmmfohfC: opodf;
	out(d, (fod(fyq(h, SC), ql(Q)), tfod(opodf_up_bitstring(dibmmfohfC), L)));
	in(d, n4: bitstring);
	let (dibmmfohfB: opodf, =dibmmfohfC) = tefd(n4, L) in
	out(d, tfod(opodf_up_bitstring(dibmmfohfB), L));
	in(d, n5: bitstring);
	let gs = tefd(n5, L) in
	if wfsif(gs,ql(Q),L) = true then
	0.

