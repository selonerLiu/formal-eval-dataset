




type skey.
type pkey.
type seed.
type passwd.



fun pk(skey): pkey.
fun penc(bitstring, pkey, seed): bitstring.
fun pdec(bitstring, skey): bitstring.
equation forall x: bitstring, y: skey, z: seed;  pdec(penc(x, pk(y), z), y) = x.



fun h(passwd): bitstring.

free c: channel.
weaksecret w.

process
	new s: skey;
	new a: seed;
	out(c, pk(s));
	out(c, penc(h(w), pk(s), a))


