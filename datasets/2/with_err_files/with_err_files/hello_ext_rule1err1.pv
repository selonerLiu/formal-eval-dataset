(* hello_ext.pv: Hello Extended World Script *)

free c:channel.

free Cocks:bitstring [private].
free RSA:bitstring [private].

event evCocks.
event evRSA.

query event(evCocks) ==> event(evRSA).

process
	out(c,RSA);
	in(c,x:bitstring);
	f x = Cocks then
		event evCocks;
		event evRSA
	else
		event evRSA
