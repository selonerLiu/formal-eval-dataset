free s:bitstring [private].

table tbl(bitstring).

fun enc(bitstring,bitstring):bitstring.
reduc forall x:bitstring,y:bitstring; dec(enc(x,y),y) = x.

set preciseActions = true.


process
  new k1:bitstring;
  new k2:bitstring;
  insert tbl(k1);
  insert tbl(k2);
  get tbl(x:bitstring) in
  out(c,x);
  out(c,choice[enc(s,(k1,k2)),s])


